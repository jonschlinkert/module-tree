{
  "name": "module-tree",
  "description": "Create a visual tree of module dependencies using archy. Optionally pass a glob pattern to limit the result to names with that pattern, or a regex to selectively highlight package names.",
  "version": "0.1.2",
  "homepage": "https://github.com/jonschlinkert/module-tree",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/module-tree",
  "bugs": {
    "url": "https://github.com/jonschlinkert/module-tree/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "cli.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "mtree": "cli.js"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-yellow": "^0.1.1",
    "archy": "^1.0.0",
    "define-property": "^0.2.5",
    "glob-object": "^0.1.2",
    "lazy-cache": "^1.0.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "pkg",
    "tree"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": ["normalize-pkg"]
    },
    "reflinks": [
      "verb",
      "glob-object",
      "archy"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
